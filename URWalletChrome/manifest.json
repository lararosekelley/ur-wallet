{
  "manifest_version": 2,

  "name": "UR Wallet",
  "description": "This extension takes the essential information from BlackBoard and displays it.",
  "version": "1",

  "permissions": [
    "tabs", 
    "*://my.rochester.edu/*",
    "*://ecard.sequoiars.com/*",
    "storage",
    "management"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}