{
  "manifest_version": 2,
  "name": "UR Wallet",
  "description": "A Tool for University of Rochester Students.",
  "version": "1.0",

  "permissions": [
    "http://*/*", 
    "https://*/*",
    "*://my.rochester.edu/*",
    "*://ecard.sequoiars.com/*",
    "storage",
    "management"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}